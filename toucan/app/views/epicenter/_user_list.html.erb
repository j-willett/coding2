<div class="row">
  <% @users.each do |user| %>
    <div class="col-md-4">
      <div class="well">
        <h2><%= link_to user.username, show_user_path(id: user.id) %></h2>
        <h3><%= user.name %></h3>
        <p>
          <%= user.bio %>
        </p>
      </div>
    </div>
  <% end %>
</div>
